
[Model(ORawrM::NullBacking)]
model Tag
{
	[Key]
	guid pid;

	string name;
};

[Model(ORawrM::NullBacking)]
model Comment
{
	[Key]
	guid pid;

	int64 dateCreated;
	string contents;
};


[Model(ORawrM::NullBacking)]
model TagBridge
{
	[Key]
	guid pid;

	[ParentLink(Image, tags)]
	guid parent;

	guid tagReference;
};

[Model(ORawrM::NullBacking)]
model Image
{
	[Key]
	guid pid;

	string filehash;
	string mime; 
	int64 uploadedDate;

	set<TagBridge> tags;

	[Owns]
	set<Comment> comments;
};






